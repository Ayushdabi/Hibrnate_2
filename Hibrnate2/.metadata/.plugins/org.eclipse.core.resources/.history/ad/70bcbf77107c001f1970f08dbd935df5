public class TestDelete {
	public static void main(String[] args) {

		UserDTO dto = new UserDTO();
		dto.setId(2);
		dto.setFirstName("abc");
		dto.setLastName("abc");
		dto.setLoginId("abc@gmail.com");
		dto.setPassword("123");
		dto.setDob(new Date());
